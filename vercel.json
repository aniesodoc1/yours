{
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "https://yours-server.vercel.app/api/:path*"
      }
    ],
    "redirects": [
      {
        "source": "/socket.io/:path*",
        "destination": "https://yours-socket.vercel.app/socket.io/:path*",
        "permanent": false
      }
    ]
  }
  